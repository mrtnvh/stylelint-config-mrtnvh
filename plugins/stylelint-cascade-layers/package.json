{
  "name": "stylelint-cascade-layers",
  "description": "Stylelint plugin to enforce usage of cascade layers",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- -w",
    "build": "pkgroll",
    "lint": "stylelint -c src/__tests__/stylelint.test.config.cjs src/__tests__/*.css",
    "test": "node --test",
    "test:watch": "node --test --watch"
  },
  "peerDependencies": {
    "stylelint": "^15.11.0"
  },
  "devDependencies": {
    "pkgroll": "^2.0.2",
    "snapshot-assertion": "^5.0.0",
    "stylelint": "^15.11.0",
    "typescript": "^5.4.2"
  },
  "volta": {
    "node": "20.8.0"
  }
}
